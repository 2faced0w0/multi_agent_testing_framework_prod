{
  "annotations": {"list": []},
  "editable": true,
  "panels": [
    {
      "type": "stat",
      "title": "App Up",
      "targets": [
        {"expr": "app_info{job=~\".*matf.*\"}", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Health Latency (ms)",
      "targets": [
        {"expr": "sum(rate(http_server_requests_seconds_sum{path=\"/api/v1/system/health\"}[5m])) / sum(rate(http_server_requests_seconds_count{path=\"/api/v1/system/health\"}[5m])) * 1000", "refId": "A"}
      ]
    },
    {
      "type": "stat",
      "title": "Generated Tests",
      "targets": [
        {"expr": "tests_generated_total", "refId": "A"}
      ]
    },
    {
      "type": "stat",
      "title": "Executed Tests",
      "targets": [
        {"expr": "tests_executed_total", "refId": "A"}
      ]
    }
  ],
  "schemaVersion": 36,
  "version": 1,
  "title": "MATF Overview"
}
